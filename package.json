{
  "name": "react-skeleton-boilerplate",
  "description": "A React ES6 skeleton boilerplate",
  "version": "0.0.8",
  "license": "BSD-2-Clause",
  "repository": {
    "type": "git",
    "url": "git://github.com/benmanu/react-skeleton.git"
  },
  "author": "Ben Manu",
  "contributors": [
    "Ben Manu",
    "Tim Kung"
  ],
  "main": "./dist/index.js",
  "scripts": {
    "build": "npm run js",
    "js": "browserify -t [ babelify ] lib/app.js -o dist/index.js",
    "lint": "eslint lib/**",
    "test": "jest",
    "shrinkwrap": "npm-shrinkwrap"
  },
  "dependencies": {
    "lodash": "^4.13.1",
    "npm-shrinkwrap": "^200.5.1",
    "react": "^15.2.1",
    "react-dom": "^15.2.1",
    "reactcss": "^1.0.4"
  },
  "devDependencies": {
    "babel-jest": "^13.2.2",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-2": "^6.11.0",
    "babelify": "^7.3.0",
    "browserify": "^13.0.1",
    "eslint": "^2.9.0",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.11.1",
    "eslint-plugin-jsx-a11y": "^1.2.0",
    "eslint-plugin-react": "^5.2.2",
    "jest-cli": "^13.2.3",
    "react-addons-test-utils": "^15.2.1"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "jest": {
    "unmockedModulePathPatterns": [
      "node_modules/react/",
      "node_modules/react-dom/",
      "node_modules/react-addons-test-utils/",
      "node_modules/reactcss"
    ]
  }
}
