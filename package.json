{
  "name": "react-skeleton",
  "description": "A React ES6 skeleton boilerplate",
  "version": "0.0.1",
  "license": "BSD-2-Clause",
  "repository": {
    "type": "git",
    "url": "git://github.com/benmanu/react-skeleton.git"
  },
  "author": "Ben Manu",
  "contributors": [
    "Ben Manu",
    "Tim Kung"
  ],
  "main": "./lib/app.js",
  "scripts": {
    "build": "npm run js",
    "js": "browserify -t [ babelify ] lib/app.js -o index.js",
    "js:prod": "browserify -t [ babelify ] -t [ envify --NODE_ENV production ] lib/app.js -o index.js",
    "lint": "eslint lib/**"
  },
  "dependencies": {
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-2": "^6.11.0",
    "babelify": "^7.3.0",
    "browserify": "^13.0.1",
    "envify": "^3.4.1",
    "lodash": "^4.13.1",
    "react": "^15.2.1",
    "react-dom": "^15.2.1",
    "react-skeleton-boilerplate": "0.0.5"
  },
  "devDependencies": {
    "eslint": "^2.9.0",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.11.1",
    "eslint-plugin-jsx-a11y": "^1.2.0",
    "eslint-plugin-react": "^5.2.2",
    "react-addons-test-utils": "^15.2.1"
  }
}
